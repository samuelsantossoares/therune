local ProtecaoTab = Window:MakeTab({
    Name = "Prote√ß√£o",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

-- Vari√°veis de prote√ß√£o
local antiSitEnabled = false
local antiVoidEnabled = false
local antiFlingEnabled = false

-- Anti Sit
ProtecaoTab:AddSection({Name = "ü™ë Anti Sit"})

ProtecaoTab:AddToggle({
    Name = "Anti Sit",
    Default = false,
    Callback = function(Value)
        antiSitEnabled = Value
        if antiSitEnabled then
            OrionLib:MakeNotification({
                Name = "Anti Sit Ativado",
                Content = "Voc√™ n√£o ser√° mais for√ßado a sentar",
                Time = 3
            })
            
            -- Prevenir que seja for√ßado a sentar
            LocalPlayer.CharacterAdded:Connect(function(character)
                local humanoid = character:WaitForChild("Humanoid")
                humanoid:SetStateEnabled(Enum.HumanoidStateType.Seated, false)
            end)
            
            if LocalPlayer.Character then
                local humanoid = LocalPlayer.Character:FindFirstChild("Humanoid")
                if humanoid then
                    humanoid:SetStateEnabled(Enum.HumanoidStateType.Seated, false)
                end
            end
        else
            OrionLib:MakeNotification({
                Name = "Anti Sit Desativado",
                Content = "Prote√ß√£o anti-sit desligada",
                Time = 3
            })
        end
    end
})

-- Anti Void
ProtecaoTab:AddSection({Name = "üï≥Ô∏è Anti Void"})

ProtecaoTab:AddToggle({
    Name = "Anti Void",
    Default = false,
    Callback = function(Value)
        antiVoidEnabled = Value
        if antiVoidEnabled then
            OrionLib:MakeNotification({
                Name = "Anti Void Ativado",
                Content = "Prote√ß√£o contra void ativada",
                Time = 3
            })
            
            local function checkPosition()
                while antiVoidEnabled do
                    if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                        local position = LocalPlayer.Character.HumanoidRootPart.Position
                        -- Se cair no void, teleportar de volta
                        if position.Y < -500 then
                            LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(0, 50, 0)
                            OrionLib:MakeNotification({
                                Name = "Anti Void",
                                Content = "Voc√™ foi salvo do void!",
                                Time = 4
                            })
                        end
                    end
                    wait(1)
                end
            end
            
            spawn(checkPosition)
        else
            OrionLib:MakeNotification({
                Name = "Anti Void Desativado",
                Content = "Prote√ß√£o contra void desligada",
                Time = 3
            })
        end
    end
})

-- Anti Fling
ProtecaoTab:AddSection({Name = "üåÄ Anti Fling"})

ProtecaoTab:AddToggle({
    Name = "Anti Fling",
    Default = false,
    Callback = function(Value)
        antiFlingEnabled = Value
        if antiFlingEnabled then
            OrionLib:MakeNotification({
                Name = "Anti Fling Ativado",
                Content = "Prote√ß√£o contra fling ativada",
                Time = 3
            })
            
            local function antiFling()
                while antiFlingEnabled do
                    if LocalPlayer.Character then
                        for _, part in pairs(LocalPlayer.Character:GetDescendants()) do
                            if part:IsA("BodyVelocity") or part:IsA("BodyAngularVelocity") then
                                part:Destroy()
                            end
                        end
                    end
                    wait(0.1)
                end
            end
            
            spawn(antiFling)
        else
            OrionLib:MakeNotification({
                Name = "Anti Fling Desativado",
                Content = "Prote√ß√£o contra fling desligada",
                Time = 3
            })
        end
    end
})

