local playerList = {}
local playerDropdown = EntryExitTab:AddDropdown({
    Name = "Selecionar Jogador",
    Default = "Nenhum",
    Options = {"Nenhum"},
    Callback = function(Value)
        trackedPlayer = Value == "Nenhum" and nil or Value
    end
})

local function updatePlayerList()
    local playerNames = {"Nenhum"}
    playerList = {}
    
    for _, player in pairs(Players:GetPlayers()) do
        if player ~= Players.LocalPlayer then
            table.insert(playerNames, player.Name)
            playerList[player.Name] = player
        end
    end
    
    playerDropdown:Refresh(playerNames, true)
end

EntryExitTab:AddToggle({
    Name = "Rastrear Jogador Espec√≠fico",
    Default = false,
    Callback = function(Value)
        playerTrackingEnabled = Value
        OrionLib:MakeNotification({
            Name = "Rastreamento",
            Content = Value and "Ativado!" or "Desativado!",
            Time = 3
        })
    end
})

EntryExitTab:AddButton({
    Name = "Atualizar Lista de Jogadores",
    Callback = function()
        updatePlayerList()
        OrionLib:MakeNotification({
            Name = "Lista Atualizada",
            Content = "Lista de jogadores atualizada!",
            Time = 2
        })
    end
})

local function setupPlayerNotifications()
    Players.PlayerAdded:Connect(function(player)
        if ownerTrackingEnabled and player.UserId == ownerId then
            OrionLib:MakeNotification({
                Name = "üåü DONO ENTOU!",
                Content = ownerName .. " entrou no servidor!",
                Time = 10,
                Image = "rbxassetid://4483345998"
            })
        elseif playerTrackingEnabled and trackedPlayer and player.Name == trackedPlayer then
            OrionLib:MakeNotification({
                Name = "Jogador Rastreado Entrou",
                Content = trackedPlayer .. " entrou no servidor!",
                Time = 5
            })
        else
            OrionLib:MakeNotification({
                Name = "Jogador Entrou",
                Content = player.Name .. " entrou no servidor",
                Time = 3
            })
        end
        wait(1)
        updatePlayerList()
    end)
    
    Players.PlayerRemoving:Connect(function(player)
        if ownerTrackingEnabled and player.UserId == ownerId then
            OrionLib:MakeNotification({
                Name = "üåü DONO SAIU!",
                Content = ownerName .. " saiu do servidor!",
                Time = 10,
                Image = "rbxassetid://4483345998"
            })
        elseif playerTrackingEnabled and trackedPlayer and player.Name == trackedPlayer then
            OrionLib:MakeNotification({
                Name = "Jogador Rastreado Saiu",
                Content = trackedPlayer .. " saiu do servidor!",
                Time = 5
            })
        else
            OrionLib:MakeNotification({
                Name = "Jogador Saiu",
                Content = player.Name .. " saiu do servidor",
                Time = 3
            })
        end
        wait(1)
        updatePlayerList()
    end)
end

-- ========== TAB TROLL ==========
local TrollTab = Window:MakeTab({
    Name = "Troll",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

-- Vari√°veis para Player Troll
local selectedPlayerName = nil

-- Se√ß√£o Player Troll
TrollTab:AddSection({Name = "üéØ Selecionar Jogador"})

local function getPlayerList()
    local players = Players:GetPlayers()
    local playerNames = {}
    for _, player in ipairs(players) do
        if player ~= LocalPlayer then
            table.insert(playerNames, player.Name)
        end
    end
    return playerNames
end

local killDropdown = TrollTab:AddDropdown({
    Name = "Selecionar Jogador",
    Options = getPlayerList(),
    Default = "",
    Callback = function(value)
        selectedPlayerName = value
        getgenv().Target = value
        OrionLib:MakeNotification({
            Name = "Jogador Selecionado",
            Content = "Alvo: " .. value,
            Time = 2
        })
    end
})

TrollTab:AddButton({
    Name = "Atualizar Lista de Jogadores",
    Callback = function()
        local newPlayers = getPlayerList()
        killDropdown:Refresh(newPlayers, true)
        OrionLib:MakeNotification({
            Name = "Lista Atualizada",
            Content = "Lista de jogadores atualizada!",
            Time = 2
        })
    end
})

-- Fun√ß√µes b√°sicas
TrollTab:AddSection({Name = "üîß Fun√ß√µes B√°sicas"})

TrollTab:AddButton({
    Name = "Teleportar para o Jogador",
    Callback = function()
        if not selectedPlayerName then
            OrionLib:MakeNotification({
                Name = "Erro",
                Content = "Nenhum jogador selecionado!",
                Time = 3
            })
            return
        end
        
        local character = LocalPlayer.Character
        local humanoidRootPart = character and character:FindFirstChild("HumanoidRootPart")
        if not humanoidRootPart then
            OrionLib:MakeNotification({
                Name = "Erro",
                Content = "HumanoidRootPart n√£o encontrado!",
                Time = 3
            })
            return
        end

        local targetPlayer = Players:FindFirstChild(selectedPlayerName)
        if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
            humanoidRootPart.CFrame = targetPlayer.Character.HumanoidRootPart.CFrame
            OrionLib:MakeNotification({
                Name = "Teleport Conclu√≠do",
                Content = "Teleportado para: " .. selectedPlayerName,
                Time = 3
            })
        else
            OrionLib:MakeNotification({
                Name = "Erro",
                Content = "Jogador alvo n√£o encontrado!",
                Time = 3
            })
        end
    end
})

TrollTab:AddToggle({
    Name = "Spectate Player",
    Default = false,
    Callback = function(value)
        local Camera = workspace.CurrentCamera

        if value then
            if not selectedPlayerName then
                OrionLib:MakeNotification({
                    Name = "Erro",
                    Content = "Selecione um jogador primeiro!",
                    Time = 3
                })
                return
            end
            
            local targetPlayer = Players:FindFirstChild(selectedPlayerName)
            if targetPlayer and targetPlayer.Character then
                local humanoid = targetPlayer.Character:FindFirstChild("Humanoid")
                if humanoid then
                    Camera.CameraSubject = humanoid
                    OrionLib:MakeNotification({
                        Name = "Spectate Ativado",
                        Content = "Observando: " .. selectedPlayerName,
                        Time = 3
                    })
                end
            end
        else
            if LocalPlayer.Character then
                local humanoid = LocalPlayer.Character:FindFirstChild("Humanoid")
                if humanoid then
                    Camera.CameraSubject = humanoid
                    OrionLib:MakeNotification({
                        Name = "Spectate Desativado",
                        Content = "Modo espectador desligado",
                        Time = 3
                    })
                end
            end
        end
    end
})

-- Sistema Couch
TrollTab:AddSection({Name = "üõãÔ∏è Sistema Couch"})

local function cleanupCouch()
    local char = LocalPlayer.Character
    if char then
        local couch = char:FindFirstChild("Chaos.Couch") or LocalPlayer.Backpack:FindFirstChild("Chaos.Couch")
        if couch then
            couch:Destroy()
        end
    end
    pcall(function()
        ReplicatedStorage:WaitForChild("RE"):WaitForChild("1Clea1rTool1s"):FireServer("ClearAllTools")
    end)
end

LocalPlayer.CharacterAdded:Connect(function(newCharacter)
    local Humanoid = newCharacter:WaitForChild("Humanoid")
    cleanupCouch()
    Humanoid.Died:Connect(cleanupCouch)
end)

local function KillPlayerCouch()
    if not selectedPlayerName then
        OrionLib:MakeNotification({
            Name = "Erro",
            Content = "Nenhum jogador selecionado!",
            Time = 3
        })
        return
    end
    
    local target = Players:FindFirstChild(selectedPlayerName)
    if not target or not target.Character then
        OrionLib:MakeNotification({
            Name = "Erro",
            Content = "Jogador alvo n√£o encontrado!",
            Time = 3
        })
        return
    end

    local char = LocalPlayer.Character
    if not char then
        OrionLib:MakeNotification({
            Name = "Erro",
            Content = "Personagem local n√£o encontrado!",
            Time = 3
        })
        return
    end
    
    local hum = char:FindFirstChildOfClass("Humanoid")
    local root = char:FindFirstChild("HumanoidRootPart")
    local tRoot = target.Character:FindFirstChild("HumanoidRootPart")
    
    if not hum or not root or not tRoot then
        OrionLib:MakeNotification({
            Name = "Erro",
            Content = "Componentes necess√°rios n√£o encontrados!",
            Time = 3
        })
        return
    end

    local originalPos = root.Position 
    local sitPos = Vector3.new(145.51, -350.09, 21.58)

    OrionLib:MakeNotification({
        Name = "Iniciando Kill Couch",
        Content = "Preparando sistema...",
        Time = 3
    })

    pcall(function()
        ReplicatedStorage:WaitForChild("RE"):WaitForChild("1Clea1rTool1s"):FireServer("ClearAllTools")
    end)
    task.wait(0.5)

    pcall(function()
        ReplicatedStorage.RE:FindFirstChild("1Too1l"):InvokeServer("PickingTools", "Couch")
    end)
    task.wait(0.5)

    local tool = LocalPlayer.Backpack:FindFirstChild("Couch")
    if tool then 
        tool.Parent = char 
        OrionLib:MakeNotification({
            Name = "Couch Equipado",
            Content = "Couch equipado com sucesso",
            Time = 2
        })
    else
        OrionLib:MakeNotification({
            Name = "Erro",
            Content = "Couch n√£o encontrado na mochila!",
            Time = 3
        })
        return
    end
    task.wait(0.5)

    pcall(function()
        VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.F, false, game)
    end)
    task.wait(0.5)

    hum:SetStateEnabled(Enum.HumanoidStateType.Seated, false)
    hum.PlatformStand = false

    local align = Instance.new("BodyPosition")
    align.Name = "BringPosition"
    align.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
    align.D = 10
    align.P = 30000
    align.Position = root.Position
    align.Parent = tRoot

    OrionLib:MakeNotification({
        Name = "Kill em Andamento",
        Content = "Perseguindo: " .. selectedPlayerName,
        Time = 4
    })

    task.spawn(function()
        local angle = 0
        local startTime = tick()
        
        while tick() - startTime < 8 and target and target.Character and target.Character:FindFirstChildOfClass("Humanoid") do
            local tHum = target.Character:FindFirstChildOfClass("Humanoid")
            if not tHum or tHum.Sit then break end

            local hrp = target.Character.HumanoidRootPart
            local adjustedPos = hrp.Position + (hrp.Velocity / 1.5)

            angle += 50
            root.CFrame = CFrame.new(adjustedPos + Vector3.new(0, 2, 0)) * CFrame.Angles(math.rad(angle), 0, 0)
            align.Position = root.Position + Vector3.new(2, 0, 0)

            task.wait()
        end

        align:Destroy()
        hum:SetStateEnabled(Enum.HumanoidStateType.Seated, true)
        hum.PlatformStand = false

        for _, p in pairs(char:GetDescendants()) do
            if p:IsA("BasePart") then
                p.Velocity = Vector3.zero
                p.RotVelocity = Vector3.zero
            end
        end

        task.wait(0.2)
        root.CFrame = CFrame.new(sitPos)
        task.wait(0.5)

        local tool = char:FindFirstChild("Couch")
        if tool then 
            tool.Parent = LocalPlayer.Backpack 
        end

        task.wait(0.1)
        pcall(function()
            ReplicatedStorage.RE:FindFirstChild("1Too1l"):InvokeServer("PickingTools", "Couch")
        end)
        task.wait(0.3)
        root.CFrame = CFrame.new(originalPos)
        
        OrionLib:MakeNotification({
            Name = "‚úÖ Kill Conclu√≠do",
            Content = "Kill com Couch finalizado!",
            Time = 4
        })
    end)
end

local function BringPlayerLLL()
    if not selectedPlayerName then
        OrionLib:MakeNotification({
            Name = "Erro",
            Content = "Nenhum jogador selecionado!",
            Time = 3
        })
        return
    end
    
    local target = Players:FindFirstChild(selectedPlayerName)
    if not target or not target.Character then
        OrionLib:MakeNotification({
            Name = "Erro",
            Content = "Jogador alvo n√£o encontrado!",
            Time = 3
        })
        return
    end

    local char = LocalPlayer.Character
    if not char then
        OrionLib:MakeNotification({
            Name = "Erro",
            Content = "Personagem local n√£o encontrado!",
            Time = 3
        })
        return
    end
    
    local hum = char:FindFirstChildOfClass("Humanoid")
    local root = char:FindFirstChild("HumanoidRootPart")
    local tRoot = target.Character:FindFirstChild("HumanoidRootPart")
    
    if not hum or not root or not tRoot then
        OrionLib:MakeNotification({
            Name = "Erro",
            Content = "Componentes necess√°rios n√£o encontrados!",
            Time = 3
        })
        return
    end

    local originalPos = root.Position 

    OrionLib:MakeNotification({
        Name = "Iniciando Bring Couch",
        Content = "Trazendo jogador...",
        Time = 3
    })

    pcall(function()
        ReplicatedStorage:WaitForChild("RE"):WaitForChild("1Clea1rTool1s"):FireServer("ClearAllTools")
    end)
    task.wait(0.5)

    pcall(function()
        ReplicatedStorage.RE:FindFirstChild("1Too1l"):InvokeServer("PickingTools", "Couch")
    end)
    task.wait(0.5)

    local tool = LocalPlayer.Backpack:FindFirstChild("Couch")
    if tool then 
        tool.Parent = char 
    end
    task.wait(0.5)

    pcall(function()
        VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.F, false, game)
    end)
    task.wait(0.5)

    hum:SetStateEnabled(Enum.HumanoidStateType.Seated, false)
    hum.PlatformStand = false

    local align = Instance.new("BodyPosition")
    align.Name = "BringPosition"
    align.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
    align.D = 10
    align.P = 30000
    align.Position = root.Position
    align.Parent = tRoot

    task.spawn(function()
        local angle = 0
        local startTime = tick()
        
        while tick() - startTime < 8 and target and target.Character and target.Character:FindFirstChildOfClass("Humanoid") do
            local tHum = target.Character:FindFirstChildOfClass("Humanoid")
            if not tHum or tHum.Sit then break end

            local hrp = target.Character.HumanoidRootPart
            local adjustedPos = hrp.Position + (hrp.Velocity / 1.5)

            angle += 50
            root.CFrame = CFrame.new(adjustedPos + Vector3.new(0, 2, 0)) * CFrame.Angles(math.rad(angle), 0, 0)
            align.Position = root.Position + Vector3.new(2, 0, 0)

            task.wait()
        end

        align:Destroy()
        hum:SetStateEnabled(Enum.HumanoidStateType.Seated, true)
        hum.PlatformStand = false

        for _, p in pairs(char:GetDescendants()) do
            if p:IsA("BasePart") then
                p.Velocity = Vector3.zero
                p.RotVelocity = Vector3.zero
            end
        end

        task.wait(0.2)
        root.Anchored = true
        root.CFrame = CFrame.new(originalPos)
        task.wait(0.1)
        root.Anchored = false

        task.wait(0.7)
        local tool = char:FindFirstChild("Couch")
        if tool then
            tool.Parent = LocalPlayer.Backpack
        end

        task.wait(0.1)
        pcall(function()
            ReplicatedStorage.RE:FindFirstChild("1Too1l"):InvokeServer("PickingTools", "Couch")
        end)
        
        OrionLib:MakeNotification({
            Name = "‚úÖ Bring Conclu√≠do",
            Content = "Jogador trazido com sucesso!",
            Time = 4
        })
    end)
end

