local TextChatService = game:GetService("TextChatService")
local LocalPlayer = Players.LocalPlayer

-- Fun√ß√£o para detectar o executor automaticamente
local function detectExecutor()
    if identifyexecutor then
        local executor = identifyexecutor()
        if executor then
            return executor
        end
    end
    
    if getexecutorname then
        return getexecutorname()
    elseif syn and syn.request then
        return "Synapse X"
    elseif PROTOSMASHER_LOADED then
        return "ProtoSmasher"
    elseif Krnl then
        return "Krnl"
    elseif fluxus then
        return "Fluxus"
    elseif is_sirhurt_closure then
        return "SirHurt"
    elseif pepeg then
        return "Pepe Executor"
    else
        return "Executor Desconhecido"
    end
end

local executorName = detectExecutor()

-- Enviar mensagem no chat automaticamente
local function sendChatMessage()
    local playerCount = #Players:GetPlayers()
    local message = "[‚òØÔ∏è] Therune Executado com sucesso. Desenvolvido por VelhoJogador e Wakizashiii."
    local verificationMessage = "[‚òØÔ∏è] Verificamento do Therune: Foi verificado que " .. playerCount .. " jogadores est√£o presentes no servidor no momento."
    
    -- Tentar o novo sistema de chat primeiro
    local success = pcall(function()
        if TextChatService then
            local channel = TextChatService.TextChannels.RBXGeneral
            if channel then
                channel:SendAsync(message)
                wait(1)
                channel:SendAsync(verificationMessage)
                return true
            end
        end
        return false
    end)
    
    -- Se o novo sistema falhar, tentar o sistema legado
    if not success then
        pcall(function()
            game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(message, "All")
            wait(1)
            game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(verificationMessage, "All")
        end)
    end
end

-- Enviar mensagem ap√≥s um pequeno delay
spawn(function()
    wait(3)
    sendChatMessage()
end)

-- Trocar automaticamente o nome e bio do RP
local function setupTheruneProfile()
    local nameArgs = {"RolePlayName", "Therune User"}
    local bioArgs = {"RolePlayBio", "By: Wakizashiii"}
    
    local success1 = pcall(function()
        game:GetService("ReplicatedStorage"):WaitForChild("RE"):WaitForChild("1RPNam1eTex1t"):FireServer(unpack(nameArgs))
    end)
    
    local success2 = pcall(function()
        game:GetService("ReplicatedStorage"):WaitForChild("RE"):WaitForChild("1RPNam1eTex1t"):FireServer(unpack(bioArgs))
    end)
    
    return success1 and success2
end

local success = pcall(setupTheruneProfile)

