-- Sistema RGB Character
local colors = { "Bright red", "Lime green", "Bright blue", "Bright yellow", "Bright cyan", "Hot pink", "Royal purple" }
local rgbEnabled = false

local function changeColor(color)
    pcall(function()
        local args = { color }
        game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("ChangeBodyColor"):FireServer(unpack(args))
    end)
end

TrollTab:AddToggle({
    Name = "Ativar RGB Character",
    Default = false,
    Callback = function(Value)
        rgbEnabled = Value
        if rgbEnabled then
            spawn(function()
                while rgbEnabled do
                    for _, color in ipairs(colors) do
                        if not rgbEnabled then break end
                        changeColor(color)
                        wait(0.5)
                    end
                end
            end)
            OrionLib:MakeNotification({
                Name = "RGB Ativado",
                Content = "Seu personagem est√° em RGB!",
                Time = 3
            })
        else
            OrionLib:MakeNotification({
                Name = "RGB Desativado",
                Content = "RGB Character desativado",
                Time = 3
            })
        end
    end
})

